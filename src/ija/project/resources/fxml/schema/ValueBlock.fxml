<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import ija.project.ui.control.Spacer?>
<fx:root onMouseDragged="#onMouseDragged"
		 onMousePressed="#onMousePressed"
		 onMouseReleased="#onMouseReleased"
		 minHeight="40.0" minWidth="60.0"
		 type="javafx.scene.layout.BorderPane"
		 xmlns="http://javafx.com/javafx"
		 xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<VBox>
			<children>
				<HBox>
					<Spacer/>
					<Label fx:id="displayNameLabel" alignment="CENTER"/>
					<Spacer/>
				</HBox>
				<HBox>
					<Spacer />
					<GridPane fx:id="valuesGrid" styleClass="value-label">

					</GridPane>
					<Spacer />
				</HBox>
			</children>
		</VBox>
	</center>
</fx:root>
