<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ToolBar prefHeight="40.0" VBox.vgrow="NEVER">
			<items>
            	<ToggleButton mnemonicParsing="false" text="Remove" fx:id="toolRemove" />
			</items>
		</ToolBar>
		<ScrollPane>
			<content>
				<AnchorPane fx:id="schemaPane" onMouseClicked="#onMouseClicked" onDragDropped="#onDragDropped" onDragOver="#onDragOver" prefHeight="3000.0" prefWidth="3000.0">

				</AnchorPane>
			</content>
		</ScrollPane>
	</children>
</fx:root>
